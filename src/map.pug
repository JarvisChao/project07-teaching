doctype html
html(lang="en")
  head
    include includes/_config.pug
    include includes/_head.pug
    
    meta(property="og:url" content=`${baseUrl}/map.html`)
    meta(property="og:image" content=`${baseUrl}/img/og/og.jpg`)
    //- 網頁描述
    meta(property="og:description" content="輸入單一頁面的網頁描述")
    meta(name="description" content="輸入單一頁面的網頁描述")

    meta(property="og:title" content=`地圖導覽｜${title}`)
    title 地圖導覽｜#{title}

  body
    include includes/_navbar.pug

    header.l-header-inner(data-aos="fade-down")
      include mixins/_blockade.pug
      +blockade('l-header-inner__blockade')
      h2.u-title-lg.l-header-inner__title MAP

    main.l-main
      section.l-map
        .container
          .l-map__wrap
            iframe.l-map__google(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3682.9061297135568!2d120.27933465080466!3d22.619980285085497!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x346e047620e1c45b%3A0xe6d6d1bd1c764df2!2z6aeB5LqM6Jed6KGT54m55Y2A!5e0!3m2!1szh-TW!2stw!4v1621531731204!5m2!1szh-TW!2stw" allowfullscreen="" loading="lazy")

    include includes/_footer.pug
    include includes/_scripts.pug